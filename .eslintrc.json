{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": [2, "double", { "avoidEscape": true }],
    "react/jsx-filename-extension": "off",
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "object-curly-newline": 0,
    "react/prop-types": "skipUndeclared"
  }
}
